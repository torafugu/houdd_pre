<h1>Listing medium_terrains</h1>

<table>
  <tr>
    <th>Symbol</th>
    <th>Name</th>
    <th>Food</th>
    <th>Production</th>
    <th>Money</th>
    <th>Freq rate</th>
    <th>Continuity mod</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @medium_terrains.each do |medium_terrain| %>
  <tr>
    <td><%= medium_terrain.symbol %></td>
    <td><%= medium_terrain.name %></td>
    <td><%= medium_terrain.food %></td>
    <td><%= medium_terrain.production %></td>
    <td><%= medium_terrain.money %></td>
    <td><%= medium_terrain.freq_rate %></td>
    <td><%= medium_terrain.continuity_mod %></td>
    <td><%= link_to 'Show', medium_terrain %></td>
    <td><%= link_to 'Edit', edit_medium_terrain_path(medium_terrain) %></td>
    <td><%= link_to 'Destroy', medium_terrain, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

