<%= form_for(@medium_resource) do |f| %>
  <% if @medium_resource.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@medium_resource.errors.count, "error") %> prohibited this medium_resource from being saved:</h2>

      <ul>
      <% @medium_resource.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :symbol %><br />
    <%= f.text_field :symbol %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :medium_terrain %><br />
    <%= f.collection_select(:medium_terrain_id, MediumTerrain.find(:all), :id, :name) %>
  </div>
  <div class="field">
    <%= f.label :freq_rate %><br />
    <%= f.number_field :freq_rate %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
