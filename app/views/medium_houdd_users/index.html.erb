<h1>Listing medium_houdd_users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Admin</th>
    <th>Login</th>
    <th>Last login</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @medium_houdd_users.each do |medium_houdd_user| %>
  <tr>
    <td><%= medium_houdd_user.name %></td>
    <td><%= medium_houdd_user.admin_flg %></td>
    <td><%= medium_houdd_user.login_id %></td>
    <td><%= medium_houdd_user.last_login_at %></td>
    <td><%= link_to 'Show', medium_houdd_user %></td>
    <td><%= link_to 'Edit', edit_medium_houdd_user_path(medium_houdd_user) %></td>
    <td><%= link_to 'Destroy', medium_houdd_user, confirm: 'Are you sure?', method: :delete %></td>
    <td>
        <% unless medium_houdd_user.admin_flg %>
        <%= link_to 'MyPage', mypage_top_medium_houdd_user_path(medium_houdd_user) %>
        <% end %>
    </td>
    <td>
        <% if medium_houdd_user.admin_flg %>
        <%= link_to 'AdminPage', admin_top_medium_houdd_user_path(medium_houdd_user) %>
        <% end %>
    </td>
  </tr>
<% end %>
</table>

