<h1>Listing mostsmallroads</h1>

<table>
  <tr>
    <th>Start territory</th>
    <th>Dest territory</th>
    <th>Distance</th>
    <th>Level</th>
    <th>Remaining_period</th>
    <th></th>
    <th></th>
  </tr>

<% @mostsmallroads.each do |mostsmallroad| %>
  <tr>
    <td><%= mostsmallroad.start_territory.name %></td>
    <td><%= mostsmallroad.dest_territory.name %></td>
    <td><%= mostsmallroad.distance %></td>
    <td><%= mostsmallroad.level %></td>
    <td><%= mostsmallroad.remaining_period %></td>
    <td><%= link_to 'Show', mostsmallroad %></td>
    <td><%= link_to 'Edit', edit_mostsmallroad_path(mostsmallroad) %></td>
    <td><%= link_to 'Destroy', mostsmallroad, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mostsmallroad', new_mostsmallroad_path %>
