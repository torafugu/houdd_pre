<h1>Listing mostsmalliteminfos</h1>

<table>
  <tr>
    <th>Category</th>
    <th>Element</th>
    <th>Specie</th>
    <th>Name</th>
    <th>Num of tgt</th>
    <th>Accuracy</th>
    <th>Range</th>
    <th>Damage</th>
    <th>Ac</th>
    <th>Max dp</th>
    <th>Const period</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mostsmalliteminfos.each do |mostsmalliteminfo| %>
  <tr>
    <td><%= mostsmalliteminfo.mostsmallitemcategory.name %></td>
    <td><%= mostsmalliteminfo.mostsmallelement.name %></td>
    <td><%= mostsmalliteminfo.mostsmallmobspecie.name unless mostsmalliteminfo.mostsmallmobspecie.nil? %></td>
    <td><%= mostsmalliteminfo.name %></td>
    <td><%= mostsmalliteminfo.num_of_tgt %></td>
    <td><%= mostsmalliteminfo.accuracy %></td>
    <td><%= mostsmalliteminfo.range %></td>
    <td><%= mostsmalliteminfo.damage %></td>
    <td><%= mostsmalliteminfo.ac %></td>
    <td><%= mostsmalliteminfo.max_dp %></td>
    <td><%= mostsmalliteminfo.const_period %></td>
    <td><%= link_to 'Show', mostsmalliteminfo %></td>
    <td><%= link_to 'Edit', edit_mostsmalliteminfo_path(mostsmalliteminfo) %></td>
    <td><%= link_to 'Destroy', mostsmalliteminfo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

