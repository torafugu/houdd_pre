  </div>
  <div class="field">
    <%= f.label :phys_mod %><br />
    <%= f.text_field :phys_mod %>
  </div>
  <div class="field">
    <%= f.label :int_mod %><br />
    <%= f.text_field :int_mod %>
  </div>
  <div class="field">
    <%= f.label :prolificacy_mod %><br />
