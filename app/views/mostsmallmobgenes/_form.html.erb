<%= form_for(@mostsmallmobgene) do |f| %>
  <% if @mostsmallmobgene.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@mostsmallmobgene.errors.count, "error") %> prohibited this mostsmallmobgene from being saved:</h2>

      <ul>
      <% @mostsmallmobgene.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :mostsmallmobspecie %><br />
